  "main": "index.js",
  "scripts": {
    "clean": "rm -rf lib/*",

    "date": "echo \"build called on $(date \"+%Y-%m-%d  %H:%M:%S\")\"",
    "date-log": "echo \"build called on $(date \"+%Y-%m-%d  %H:%M:%S\")\" >> buildlog.txt",
    "build:babel": "babel src -d lib -s",
    "build": "npm run date && npm run date-log && npm run build:babel",

    "build-and-run": "npm run build:babel && node lib/index.js"
  },
